{
    "title": "Using IS on courses_avg",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "courses_dept": "c*"
                    }
                },
                {
                    "IS": {
                        "courses_dept": "*c"
                    }
                },
                {
                    "GT": {
                        "courses_pass": 850
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "courses_avg"
            ],
            "ORDER": "courses_avg"
        }
    },
    "isQueryValid": true,
            "result":[
                {"courses_dept": "cpsc", "courses_avg": 72.28},
                {"courses_dept": "cpsc", "courses_avg": 73.25}
            ]


}
